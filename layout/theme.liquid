<head>{% render 'pagefly-head' %}


  <!-- Local Theme - Version 3.0.1 by Troop Themes - http://troopthemes.com/ -->

  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width,initial-scale=1">

  {% if settings.favicon != nil %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  {% if template == 'index' %}
    <title>{{ page_title }}</title>
  {% elsif template == 'password' %}
    <title>{{ shop.name }}</title>
  {% else %}
    <title>{{ page_title }} </title>
  {% endif %}

  {% if page_description %}
    <meta name="description" content="{{ page_description }}">
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}">
  {% include 'framework--social-meta-tags' %}

  {{ content_for_header }}
{%- render 'bold-common' -%}

  {% include 'framework--css-variables' %}
  {% include 'partial--css-variables' %}
  {{ "theme.scss.css" | asset_url | stylesheet_tag }}
  {{ 'theme--customizations.scss.css' | asset_url | stylesheet_tag }}
  <link rel="stylesheet" href="https://use.typekit.net/wbp1jhr.css">

  {% assign currency_switcher_enabled = false %}

  <script>
    var theme = {};
    theme.settings = {};
    theme.translations = {};
    theme.classes = {};
    theme.utils = {};
    theme.partials = {};

    theme.shop_url = '{{ shop.url }}';
    theme.shop_currency = '{{ shop.currency }}';
    theme.money_format = '{{ shop.money_format }}';
    theme.currency_switcher_enabled = {{ currency_switcher_enabled }};

    theme.settings.cart_type = '{{ settings.cart--type }}';
    theme.settings.currency_switcher_enabled = false;

    theme.translations.cart_subtotal = '{{ "cart.subtotal" | t }}';
    theme.translations.products_sold_out_email_description = '{{ "products.sold_out.email_description" | t }}';
    theme.translations.general_search_see_all_results = '{{ "general.search.see_all_results" | t }}';
    theme.translations.mailing_list_success_message = '{{ "general.mailing_list.success_message" | t }}';
    theme.translations.mailing_list_email_blank = '{{ "general.mailing_list.email_blank" | t }}';

    theme.product_json = new Array();
    theme.variant_id_list = new Array();

    theme.product_modal = {};
    theme.quick_add = {};

    theme.map = {};
  </script>

  <script type="text/javascript">
    window.lazySizesConfig = window.lazySizesConfig || {};
    window.lazySizesConfig.expand = 1200;
    window.lazySizesConfig.loadMode = 3;
    window.lazySizesConfig.ricTimeout = 50;
  </script>

  {{ 'lazysizes.min.js' | asset_url | script_tag }}

  {{ '//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js' | script_tag }}

